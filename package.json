{
  "name": "amo-milestones",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "3.4.1",
    "dompurify": "1.0.10",
    "markdown-it": "8.4.2",
    "react": "16.8.3",
    "react-bootstrap": "0.32.4",
    "react-dom": "16.8.3",
    "react-helmet": "5.2.0",
    "react-router-dom": "4.3.1",
    "reactable": "muffinresearch/reactable"
  },
  "devDependencies": {
    "coveralls": "3.0.3",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.10.0",
    "fetch-mock": "7.3.1",
    "jest-enzyme": "7.0.1",
    "react-scripts": "2.1.8",
    "react-test-renderer": "16.8.3",
    "sinon": "7.2.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "npm run test -- --coverage  --collectCoverageFrom=src/**/*js --collectCoverageFrom=!src/index.js --collectCoverageFrom=!src/setupTests.js",
    "test-coveralls": "npm run test-coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "eject": "react-scripts eject",
    "lint": "eslint src"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
